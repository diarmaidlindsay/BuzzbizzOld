	        android:layout_height="match_parent"
	        android:layout_weight="19"
	        android:orientation="vertical" >


	        <TextView
	            android:layout_width="0dp"
	            android:layout_height="0dp"
	            android:layout_weight="22"
	            android:text="" />

	        <Button
	            android:id="@+id/reverse_dummy_button_to_hold"
	            android:layout_width="match_parent"
	            android:layout_height="0dp"
	            android:layout_weight="18"
	            android:background="@drawable/image_reverse_button_to_hold"/>

	        <TextView
	            android:layout_width="0dp"
	            android:layout_height="0dp"
	            android:layout_weight="9"
	            android:text="" />

	        <Button
	            android:id="@+id/reverse_dummy_button_to_ex"
	            android:layout_width="match_parent"
	            android:layout_height="0dp"
	            android:layout_weight="18"
	            android:background="@drawable/image_reverse_button_to_ex"/>

	        <TextView
	            android:layout_width="0dp"
	            android:layout_height="0dp"
	            android:layout_weight="9"
	            android:text="" />

	        <Button
	            android:id="@+id/reverse_dummy_button_to_in"
	            android:layout_width="match_parent"
	            android:layout_height="0dp"
	            android:layout_weight="18"
	            android:background="@drawable/image_reverse_button_to_in"/>

	        <TextView
	            android:layout_width="0dp"
	            android:layout_height="0dp"
	            android:layout_weight="6"
	            android:text="" />
	    </LinearLayout>
	</LinearLayout>




   <!-- タブ 画面 -->
