	        <Button
	            android:id="@+id/reverse_dummy_button_to_key"
	            android:layout_width="match_parent"
	            android:layout_height="0dp"
	            android:layout_weight="18"
	            android:background="@drawable/image_reverse_button_to_key"/>

	        <TextView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="10"
                android:text="" />

            <Button
                android:id="@+id/reverse_dummy_button_to_hst"
